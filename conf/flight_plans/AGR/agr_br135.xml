    <block group="Psurvey" name="P-Survey2-15 230m 10pt 45deg" strip_button="PSurv2-15-230m45d">
      <call fun="nav_survey_poly_osam_setup(WP_S2_1, 15, 230, 45)"/>
      <call fun="nav_survey_poly_osam_run()"/>
    </block>


    <block group="extra_pattern" name="Survey S1-S2 230m LO" strip_button="Svy230-LO">
      <survey_rectangle grid="230" orientation="WE" wp1="S1" wp2="S2"/>
    </block>
    <block group="extra_pattern" name="Survey S1-S2 230m NS" strip_button="Svy230-NS">
      <survey_rectangle grid="230" orientation="NS" wp1="S1" wp2="S2"/>
    </block>
    <block group="extra_pattern" name="Survey S1-S2 460m LO" strip_button="Svy460-LO">
      <survey_rectangle grid="460" orientation="WE" wp1="S1" wp2="S2"/>
    </block>
    <block group="extra_pattern" name="Survey S1-S2 460m NS" strip_button="Svy460-NS">
      <survey_rectangle grid="460" orientation="NS" wp1="S1" wp2="S2"/>
    </block>



    <block group="base_pattern" name="Land Right AF-TD" strip_button="Land right (wp AF-TD)" strip_icon="land-right.png">
      <set value="DEFAULT_CIRCLE_RADIUS" var="nav_radius"/>
      <deroute block="land"/>
    </block>
    <block group="base_pattern" name="Land Left AF-TD" strip_button="Land left (wp AF-TD)" strip_icon="land-left.png">
      <set value="-DEFAULT_CIRCLE_RADIUS" var="nav_radius"/>
      <deroute block="land"/>
    </block>
    <block name="land">
      <call fun="nav_compute_baseleg(WP_AF, WP_TD, WP__BASELEG, nav_radius)"/>
      <circle radius="nav_radius" until="NavCircleCount() > 0.5" wp="_BASELEG"/>
      <circle radius="nav_radius" until="And(NavQdrCloseTo(DegOfRad(baseleg_out_qdr)-(nav_radius/fabs(nav_radius))*10), 10 > fabs(GetPosAlt() - WaypointAlt(WP__BASELEG)))" wp="_BASELEG"/>
    </block>
    <block name="final">
      <exception cond="ground_alt + 10 > GetPosAlt()" deroute="flare"/>
      <go from="AF" hmode="route" vmode="glide" wp="TD"/>
    </block>
    <block name="flare">
      <go approaching_time="0" from="AF" hmode="route" throttle="0.0" vmode="throttle" wp="TD"/>
      <attitude roll="0.0" throttle="0.0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="Test datalink (go to wp 2)">
      <exception cond="datalink_time > 10" deroute="Standby"/>
      <go from="STDBY" hmode="route" wp="2"/>
      <go from="2" hmode="route" wp="STDBY"/>
    </block>
  </blocks>
</flight_plan>
