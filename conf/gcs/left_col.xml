<layout width="1024" height="768">
  <columns>
    <rows SIZE="400">
      <widget SIZE="300" NAME="strips"/>
      <widget NAME="alarms"/>
    </rows>
    <rows>
      <widget NAME="map2d">
        <papget type="message_field" display="text" x="12" y="24">
          <property name="scale" value="0.01"/>
          <property name="field" value="GPS_INT:pacc"/>
          <property name="format" value="Pacc:%.f"/>
          <property name="size" value="15."/>
          <property name="color" value="yellow"/>
        </papget>
        <papget type="message_field" display="text" x="11" y="45">
          <property name="scale" value="0.0139882"/>
          <property name="field" value="ROTORCRAFT_FP:psi"/>
          <property name="format" value="Psi: %.f"/>
          <property name="size" value="15."/>
          <property name="color" value="yellow"/>
        </papget>
        <papget type="message_field" display="text" x="12" y="2">
          <property name="scale" value="1."/>
          <property name="field" value="GPS_INT:numsv"/>
          <property name="format" value="Sats: %.f"/>
          <property name="size" value="15."/>
          <property name="color" value="yellow"/>
        </papget>
      </widget>
      <widget NAME="aircraft"/>
    </rows>
  </columns>
</layout>
